{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "echo 'Skipping install - done in build command'",
  "framework": null,
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/public/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}

