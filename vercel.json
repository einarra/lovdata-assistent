{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "echo 'Skipping install - done in build command'",
  "framework": null,
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/frontend/dist/assets/:path*"
    },
    {
      "source": "/vite.svg",
      "destination": "/frontend/dist/vite.svg"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/dist/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}

